package com.duff.utils{	import flash.utils.Dictionary;		public class Dictionary	{	//_____________________________________________________________ PUBLIC METHODS	public static function getKeys(d:Dictionary):Array	{		var a:Array = new Array();		for (var key:Object in d)		{			a.push(key);		}		return a;	}		public static function getValues(d:Dictionary):Array	{		var a:Array = new Array();		for each (var value:Object in d)		{			a.push(value);		}		return a;	}	}}